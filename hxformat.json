					],
					"type": "onePerLine"
				},
				{
					"conditions": [
						{
							"cond": "itemCount <= n",
							"value": 2
						}
					],
					"type": "noWrap"
				},
				{
					"conditions": [
						{
							"cond": "itemCount >= n",
							"value": 10
