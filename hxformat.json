{
	"indentation": {
		"character": "tab",
		"tabWidth": 2,
		"conditionalPolicy": "alignedNestedIncrease"
	},
	"sameLine": {
		"caseBody": "keep",
		"elseBody": "keep",
		"elseIf": "keep",
		"forBody": "keep",
		"ifBody": "keep",
		"ifElse": "keep",
		"whileBody": "keep"
	},
	"whitespace": {
		"typeHintColonPolicy": "onlyAfter",
		"bracesConfig": {
			"anonTypeBraces": {
				"openingPolicy": "around",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"blockBraces": {
				"openingPolicy": "around",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"objectLiteralBraces": {
				"openingPolicy": "around",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"typedefBraces": {
				"openingPolicy": "around",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			},
			"unknownBraces": {
				"openingPolicy": "around",
				"closingPolicy": "around",
				"removeInnerWhenEmpty": true
			}
		}
	},
	"wrapping": {
		"anonFunctionSignature": {
			"defaultWrap": "noWrap",
			"rules": [
				{
					"conditions": [
						{
							"cond": "itemCount >= n",
							"value": 7
						}
					],
					"type": "onePerLine",
					"additionalIndent": 1
				},
				{
					"conditions": [
						{
							"cond": "totalItemLength >= n",
							"value": 60
						}
					],
					"type": "onePerLine",
					"additionalIndent": 1
				},
				{
					"conditions": [
						{
							"cond": "lineLength >= n",
							"value": 80
						}
					],
					"type": "onePerLine",
					"additionalIndent": 1
				}
			]
		},
		"arrayWrap": {
